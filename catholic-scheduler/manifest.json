{
    "manifest_version": 2,

    "name": "catholic-scheduler",
    "description": "With this, you can read the original and the translated version in one window @Copyrighted By Winterlood",
    "version": "0.0.1",

    "browser_action": {
        "default_title": "catholic-scheduler",
        "default_icon": "image/icon.png"
    },

    "background": {
        "scripts": ["/script/background.js"],
        "persistent": true
    },

    "permissions": [
        "https://*/*",
        "http://*/*",
        "management",
        "storage",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ]
}
